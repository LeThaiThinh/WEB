extends ../index
include ../mixins/tableAdmin
include ../mixins/topbarAdmin
include ../mixins/reserveFormAdmin
block append topbar
    +topbarAdmin
block append sidebar-text
    .edit-text
        p THIS IS OUR LOGIN PAGE!
        br
        p
        | I&apos;m a paragraph. Click here to add your own text and edit me. It&rsquo;s easy. Just click &ldquo;Edit Text&rdquo; or double click me to add your own content and make changes to the font.    
block append sidebody
    +tableFormAdmin(tables)
block append mainbody
    .tables
    form(action="reserve/table/editTable/edit", method="post",redirect="reserve/table/editTable")
            each table in tables
                +tablesAdmin(table)
